<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plonw.sqlalchemy">

    <configlet
        title="Sqlalchemy Setting"
        action_id="sqlalchemy.setting"
        appId="plone.sqlalchemy"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@sqlalchemySettingControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
